{
  "name": "lumen-react-boilerplate",
  "version": "1.0.0",
  "description": "Laravel Lumen Framework + React",
  "keywords": [
    "framework",
    "laravel",
    "lumen",
    "react"
  ],
  "license": "MIT",
  "author": "Krishna Timilsina <bikranshu.t@gmail.com>",
  "require": {
    "php": ">=5.6.4",
    "laravel/lumen-framework": "5.4.*",
    "vlucas/phpdotenv": "~2.2",
    "tymon/jwt-auth": "1.0.0-beta.3",
    "dingo/api": "v1.0.0-beta8",
    "irazasyed/larasupport": "1.5.*"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "phpunit/phpunit": "~5.0",
    "mockery/mockery": "~0.9"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/",
      "database/"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
